{
  "devDependencies": {
    "eslint": "^4.8.0",
    "expect": "^21.2.1",
    "mocha": "^4.0.0",
    "prettier": "^1.7.4"
  },
  "dependencies": {
    "alien-node-redis-utils": "^1.2.0",
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "json-bigint": "^0.2.3",
    "redis": "^2.8.0",
    "request": "^2.81.0",
    "string-template": "^1.0.0",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "lint": "eslint *.js",
    "prettify": "prettier --single-quote --write './*.js'",
    "test": "mocha"
  },
  "prettier": {
    "single-quote": true,
    "tab-width": 4
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-unused-vars": 1
    }
  }
}
